{
  "foreach->items": "<div class=\"thumbnail width5 {% if match | length > 0 %}markedOtherMatch{% endif %} \n     {% if markedMatchOK | length > 0 %}markedMatchOK{% endif %}\n     {% if marked %}marked{% endif %}\" \n     data-name=\"items[{{index}}]\"\n     data-view=\"foreach->items\"\n     {% if match | length > 0 %} data-toggle=\"popover\" \n     data-animation=\"false\" data-trigger=\"hover\"\n     data-title=\"Woanders vielleicht besser?\" {% endif %}>\n    <div class=\"cont\">\n        <p> {{item.job_id}}</p>\n        <img class=\"galImg\" src=\"{{item.image_url}}\" alt=\"kein Bild\" />\n        <div class=\"overlay span12 \">\n            <div class=\"row-fluid height5\">\n                <a href=\"#img{{index}}\" data-name=\"zoom\" data-on=\"click\"><div class=\"tleft span6\">zoom</div></a>\n                <a data-name=\"items[{{index}}]['result']\" data-on=\"click->mark\" target=\"_blank\" data-value=\"{{item.result}}\" data-view=\"__static\"><div class=\"tright span6\">X</div></a>\n            </div>\n            <div class=\"row-fluid height5\">\n                <a href=\"#text{{index}}\" data-name=\"zoom\" data-on=\"click\"> <div class=\"bleft span6\">text</div></a>\n                <a href=\"https://www.google.com/search?q={{item.item_name |encodeURIComponent}}\" target=\"_blank\"><div class=\"bright span6\">www</div></a>\n            </div>\n        </div>\n    </div>\n\n    <p class=\"name\">{{item.item_name}}</p>\n    <div class=\"lightbox\" data-name=\"bg\" data-on=\"click\" id=\"img{{index}}\">\n        <div class=\"box\">\n            <p class=\"title\">{{item.item_name}}</p>\n            <i> {{item.item_external_category}}</i>\n            <div class=\"text\">\n                <img data-src=\"{{item.image_url | bigImg}}\" class=\"bigImg\"> \n                <p> {{item.item_description}}</p>\n            </div> \n            <div class=\"clear\"></div>\n        </div>\n    </div>\n\n    <div class=\"lightbox\"  data-name=\"bg\" data-on=\"click\" id=\"text{{index}}\">\n        <div class=\"box\" >\n            <p class=\"title\">{{item.item_name}}</p>\n            <i> {{item.item_external_category}}</i>\n            <div class=\"text\">\n                {{item.item_description}}\n            </div> \n            <div class=\"clear\"></div>\n        </div>\n    </div>\n    <div class=\"failed\"  data-name=\"items[{{index}}]['result']\" data-on=\"click->mark\" data-value=\"{{item.result}}\" data-view=\"__static\">X</div>\n    <div class=\"success\">&#10003;</div>\n</div>"
}